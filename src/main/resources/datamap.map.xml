<?xml version="1.0" encoding="utf-8"?>
<data-map xmlns="http://cayenne.apache.org/schema/10/modelMap"
		  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		  xsi:schemaLocation="http://cayenne.apache.org/schema/10/modelMap http://cayenne.apache.org/schema/10/modelMap.xsd"
		  project-version="10">
	<property name="defaultPackage" value="com.example.model"/>
	<db-entity name="person">
		<db-attribute name="id" type="BIGINT" isPrimaryKey="true" isMandatory="true" isGenerated="true"/>
		<db-attribute name="name" type="VARCHAR" length="255"/>
		<db-attribute name="address" type="VARCHAR" length="500"/>
		<db-attribute name="phone_number" type="VARCHAR" length="50"/>
		<db-generated-key>
			<db-key-generator name="DEFAULT"/>
		</db-generated-key>
	</db-entity>
	<obj-entity name="Person" className="com.example.model.Person" dbEntityName="person">
		<obj-attribute name="name" type="java.lang.String" db-attribute-path="name"/>
		<obj-attribute name="address" type="java.lang.String" db-attribute-path="address"/>
		<obj-attribute name="phoneNumber" type="java.lang.String" db-attribute-path="phone_number"/>
	</obj-entity>
</data-map>